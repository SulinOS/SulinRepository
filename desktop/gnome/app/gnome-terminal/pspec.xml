        <Archive sha1sum="2f354c5d987f87b144236874d614a49a71632a7e" >https://gitlab.gnome.org/GNOME/gnome-terminal/-/archive/3.36.1.1/gnome-terminal-3.36.1.1.tar.gz</Archive>
        <BuildDependencies>
            <Dependency>vte3</Dependency>
            <Dependency>gtk3-devel</Dependency>
            <Dependency>gconf-devel</Dependency>
            <Dependency>libglade-devel</Dependency>
            <Dependency>startup-notification-devel</Dependency>
            <Dependency>gnome-doc-utils</Dependency>
            <Dependency>docbook-xml</Dependency>
            <Dependency>libSM-devel</Dependency>
            <Dependency>libICE-devel</Dependency>
            <Dependency>atk-devel</Dependency>
            <Dependency>gdk-pixbuf-devel</Dependency>
            <Dependency>dconf-devel</Dependency>
            <Dependency>gnome-common</Dependency>
            <Dependency>libpcre2-devel</Dependency>
        </BuildDependencies>
    </Source>

    <Package>
        <Name>gnome-terminal</Name>
        <RuntimeDependencies>
            <Dependency>vte3</Dependency>
            <Dependency>gtk3</Dependency>
            <Dependency>gconf</Dependency>
            <Dependency>libglade</Dependency>
            <Dependency>startup-notification</Dependency>
