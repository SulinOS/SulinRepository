rm -rf logs
mkdir logs
export PWD=$(pwd)
dirname(){
pwd | sed "s|.*/||"
}
echo -e "getdir(){\npwd | sed \"s|.*/||\"\n}" > list
echo "export pwd=\$(pwd)" >> list
find | grep pspec.xml | sed "s/pspec.xml/ ; echo \"Build:\$(getdir)\" /" | sed "s|^./|cd |" |  sed "s=$=; inary bi --ignore-dep --ignore-sand --package 2> \$pwd/logs/\$(getdir).out.log | cat > \$pwd/logs/\$(getdir).err.log ; cd \$pwd=" >> list
bash list
